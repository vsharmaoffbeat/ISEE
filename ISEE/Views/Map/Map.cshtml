<script>

    //accordion first
    $('#accordion').on('hidden.bs.collapse', function () {

    })

    $('#accordion .accordion-toggle').click(function (e) {
        var chevState = $(e.target).siblings("i.indicator").toggleClass('fa-chevron-down fa-chevron-up');
        $("i.indicator").not(chevState).removeClass("fa-chevron-down").addClass("fa-chevron-up");
    });
    // end


    //accordion second
    $('#accordion1').on('hidden.bs.collapse', function () {

    })

    $('#accordion1 .accordion-toggle').click(function (e) {
        var chevState = $(e.target).siblings("i.indicator1").toggleClass('fa-chevron-down fa-chevron-up');
        $("i.indicator1").not(chevState).removeClass("fa-chevron-down").addClass("fa-chevron-up");
    });

    // end


    //accordion third
    $('#accordion2').on('hidden.bs.collapse', function () {

    })

    $('#accordion2 .accordion-toggle').click(function (e) {
        var chevState = $(e.target).siblings("i.indicator2").toggleClass('fa-chevron-down fa-chevron-up');
        $("i.indicator2").not(chevState).removeClass("fa-chevron-down").addClass("fa-chevron-up");
    });

    //End




    //Flip function
    $('#flip-button').on('click', function () {
        $('.side').toggleClass('rotated');
    });


    $('#flip-buttons').on('click', function () {
        $('.side').toggleClass('rotated');
    });


</script>






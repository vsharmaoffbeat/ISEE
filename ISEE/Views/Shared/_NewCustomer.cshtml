    ];
    $("#inputState").autocomplete({
        source: availableTags
    });

    var availableTags = [
    "Estado",
    "Estado",
    "Iquique",
    "Aguascalientes",
    "BajaCalifornia",
   "Campeche",
   "Coahuila de Zaragoza",
   "Colima",
   "Chiapas",
   "Chihuahua",
   "Distrito Federal",
   "Durango",
   "Guanajuato",
   "Guerrero",
   "Hidalgo",
    ];

    $("#inputCity").autocomplete({
        source: availableTags
